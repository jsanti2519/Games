#include <iostream>
using namespace std;

int main() {
    int arr[9][9]; bool repeats = true;
    arr[0][0] = (rand() % 9) + 1;
    for(int c = 1; c < 9; c++) {
        while(repeats) {
            arr[0][c] = (rand() % 9) + 1;
            for(int k = 0; k < c; k++) {
                if(arr[0][k] == arr[0][c]) {
                    repeats = true;
                    break;
                } 
                else repeats = false;
            }
        }
        cout << arr[0][c] << " ";
    }
}
